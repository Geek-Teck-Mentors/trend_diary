{
  "name": "my-app",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "prestart": "npm run supabase:start",
    "start": "react-router dev",
    "poststart": "supabase stop",
    "build": "react-router build",
    "supabase:start": "supabase start --exclude realtime,storage-api,imgproxy,postgrest,postgres-meta,studio,edge-runtime,logflare,vector,supavisor,mailpit",
    "test:frontend": "vitest run -c src/test/vitest/config/frontend.ts",
    "test:domain": "vitest run -c src/test/vitest/config/domain.ts",
    "test:api": "vitest run -c src/test/vitest/config/api.ts",
    "test:common": "vitest run -c src/test/vitest/config/common.ts",
    "e2e": "playwright test --config=src/test/e2e/playwright.config.ts",
    "e2e:report": "playwright show-report src/test/e2e/playwright-report",
    "e2e:gen": "playwright codegen --config=src/test/e2e/playwright.config.ts",
    "typecheck": "tsgo --noEmit",
    "lint": "biome ci . && npm run typecheck",
    "check": "biome check .",
    "check:fix": "biome check --write . --unsafe",
    "db:migrate": "prisma migrate dev",
    "db:migrate:sql-only": "prisma migrate dev --skip-seed --skip-generate",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx ./src/infrastructure/prisma-orm/seed.ts",
    "db:deploy": "prisma migrate deploy",
    "supabase:db:type-gen": "supabase gen types typescript --local > supabase/functions/infrastructure/database.types.ts",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "vitest run -c src/test/vitest/config/storybook.ts"
  },
  "prisma": {
    "schema": "./src/infrastructure/prisma-orm"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.5.0",
    "@prisma/client": "^6.7.0",
    "@prisma/extension-accelerate": "^1.2.2",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@react-router/cloudflare": "^7.0.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tailwindcss/vite": "^4.0.8",
    "@tanstack/react-table": "^8.21.3",
    "@yuukihayashi0510/core": "npm:@jsr/yuukihayashi0510__core@^0.2.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.10.4",
    "hono-react-router-adapter": "^0.6.5",
    "lucide-react": "^0.487.0",
    "next-themes": "^0.4.6",
    "pino": "^9.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.0.0",
    "rss-parser": "^3.13.0",
    "sonner": "^2.0.3",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.1.0",
    "tw-animate-css": "^1.2.5",
    "uuid": "^11.0.5",
    "vaul": "^1.1.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@chromatic-com/storybook": "^4.0.1",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@faker-js/faker": "^9.7.0",
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.1",
    "@playwright/test": "^1.56.1",
    "@prisma/generator-helper": "^6.5.0",
    "@react-router/dev": "^7.0.0",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-vitest": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.15.30",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^10.0.0",
    "@typescript/native-preview": "^7.0.0-dev.20251209.1",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.56.1",
    "prisma": "^7.2.0",
    "prisma-erd-generator": "^2.0.4",
    "storybook": "^9.1.17",
    "tailwindcss": "^4.0.8",
    "tsx": "^4.20.4",
    "typescript": "^5.7.3",
    "vite": "^5.4.21",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0",
    "wrangler": "^4.27.0"
  }
}
