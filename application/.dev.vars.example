# SupabaseのDB URLを設定
# edge環境ではprisma://から始まるURLにする必要がある
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Direct connection to the database. Used for migrations
DIRECT_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Discord webhook URL for alert
DISCORD_WEBHOOK_URL=

# 一時的なフィーチャーフラグ
FEATURE_USER_ENABLED=false

# Supabase設定
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=

# ログレベル(debug,info,warn,error,silent)。未記載の場合の初期値はinfo
LOG_LEVEL=
